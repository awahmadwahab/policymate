{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/public/$1"
      }
    ],
    "functions": {
      "api/**/*.js": {
        "memory": 3008,
        "maxDuration": 30
      }
    }
  }